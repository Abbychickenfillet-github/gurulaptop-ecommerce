{
  "name": "express-base-esm",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --delay 2 --watch . --ext js,json --ignore node_modules/ ./bin/www",
    "seed": "node ./cli/db-init.js",
    "backup": "node ./cli/db-backup.js",
    "debug": "DEBUG=express:* node ./bin/www",
    "debug-win": "set DEBUG=express:* & node ./bin/www",
    "start": "node ./bin/www"
  },
  "nodemonConfig": {
    "delay": "2000",
    "watch": [
      "routes",
      "bin",
      "configs",
      "db-helpers"
    ],
    "ignore": [
      "node_modules",
      "public",
      "uploads",
      "*.log"
    ],
    "ext": "js,json",
    "env": {
      "NODE_ENV": "development"
    }
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "axios": "^1.7.7",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bluebird": "^3.7.2",
    "ckeditor5": "^43.3.0",
    "colors": "^1.4.0",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "debug": "~4.3.4",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-session": "^1.18.0",
    "http-errors": "~2.0.0",
    "jsonwebtoken": "^9.0.2",
    "line-pay-merchant": "^0.9.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "~1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.16",
    "otpauth": "^9.2.4",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.2",
    "react-spinners": "^0.14.1",
    "react-toastify": "^10.0.6",
    "request": "^2.88.2",
    "secure-compare": "^3.0.1",
    "sequelize": "^6.37.7",
    "session-file-store": "^1.5.0",
    "sqlstring": "^2.3.3",
    "uuid": "^9.0.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/pg": "^8.15.5",

    "nodemon": "^3.1.0",
    "pg": "^8.16.3",
    "prisma": "^6.12.0"
  },
  "type": "module",
  "engines": {
    "yarn": ">= 1.6",
    "node": ">= 16.0"
  },
  "imports": {
    "##/*": "./*",
    "#configs/*": "./configs/*",
    "#data/*": "./data/*",
    "#db-helpers/*": "./db-helpers/*",
    "#middlewares/*": "./middlewares/*",
    "#services/*": "./services/*",
    "#utils/*": "./utils/*"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
